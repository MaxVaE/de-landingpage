{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-const-assign": 2,
    "no-unused-vars": 1,
    "no-duplicate-imports": 2,
    "class-methods-use-this": 0,
    "default-case": 0,
    "import/no-cycle": 0
  }
}
